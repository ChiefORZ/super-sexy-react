{
  "name": "super-sexy-react",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "starter",
    "boilerplate",
    "babel"
  ],
  "scripts": {
    "start": "better-npm-run start-prod",
    "dev": "better-npm-run start-dev"
  },
  "dependencies": {
    "babel-core": "6.3.17",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "better-npm-run": "0.0.5",
    "compression": "1.6.0",
    "cookie-parser": "1.4.0",
    "ejs": "2.3.4",
    "express": "4.13.3",
    "history": "1.13.1",
    "react": "0.14.3",
    "react-cookie": "0.4.3",
    "react-dom": "0.14.3",
    "react-router": "1.0.0",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-dev-server": "1.14.0"
  },
  "devDependencies": {
    "react-hot-loader": "1.3.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-prod": {
      "command": "node index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3000
      }
    }
  }
}
