{
  "name": "super-sexy-react",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "starter",
    "boilerplate",
    "babel"
  ],
  "scripts": {
    "build": "webpack --verbose --colors --display-error-details --config webpack.prod.config.js",
    "dev": "better-npm-run start-dev",
    "start": "better-npm-run start-prod",
    "postinstall": "node ./npm-scripts/postinstall.js"
  },
  "dependencies": {
    "async-props": "0.2.2",
    "autoprefixer": "6.3.1",
    "babel-core": "6.3.17",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "better-npm-run": "0.0.5",
    "classnames": "2.2.3",
    "clean-webpack-plugin": "0.1.5",
    "compression": "1.6.0",
    "cookie-parser": "1.4.0",
    "css-loader": "0.23.1",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "0.9.1",
    "history": "1.13.1",
    "node-sass": "3.4.2",
    "postcss-loader": "0.8.0",
    "react": "0.14.3",
    "react-cookie": "0.4.3",
    "react-dom": "0.14.3",
    "react-helmet": "2.2.0",
    "react-hot-loader": "1.3.0",
    "react-router": "1.0.0",
    "sass-loader": "3.1.2",
    "serialize-javascript": "1.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-dev-server": "1.14.0",
    "webpack-hot-middleware": "2.6.0",
    "webpack-isomorphic-tools": "2.2.24"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node index.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start-prod": {
      "command": "node index.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3000
      }
    }
  }
}
